<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <!-- <h1><%= title %></h1> -->

    <% if(famousPersion){ %>
    <ul class="famous_list">
      <% famousPersion.forEach(function(user){ %>
      <li>
        <a class="card" style="width: 18rem; text-decoration: none;color: black;" href="/<%= user.shortcut%>">
          <img src="<%= user.picture %>" alt="" />

          <div class="card-body">
            <h3><%= user.name%></h3>
          </div>
        </a>
      </li>
      <%});%>
    </ul>
    <%}; %>
  </body>
</html>
